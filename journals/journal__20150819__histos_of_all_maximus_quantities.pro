PRO JOURNAL__20150819__histos_of_all_maximus_quantities

  lowLim = 0.0
  highLim = 1.0e3
  logLowLim = -3
  logHighLim = 5
  cghistoplot,ALOG10(ABS(maximus.\1)),title='\1',output= do_output ? 'maxHistos--' + date + '--\1.png' : !NULL
  cghistoplot,ALOG10(ABS(maximus.\1(WHERE(maximus.\1 GT 0)))),title='\1 GT 0',output= do_output ? 'maxHistos--' + date + '--\1--LOG_GT_0.png' : !NULL
