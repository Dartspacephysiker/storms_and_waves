  PRINT,FORMAT='("In range: ",I0)',N_ELEMENTS(WHERE(maximus.\1 LE highLim AND maximus.\1 GE lowLim ))
  PRINT,''
  PRINT,FORMAT='("Out of range (log): ",I0)',N_ELEMENTS(WHERE(ALOG10(ABS(maximus.\1)) GT logHighLim OR ALOG10(ABS(maximus.\1)) LT logLowLim ))
