  PRINT,''
  PRINT,FORMAT='("Above logHighLim (log, GT 0): ",I0)',N_ELEMENTS(WHERE(ALOG10(ABS(maximus.\1(WHERE(maximus.\1 GT 0)))) GT logHighLim))
  PRINT,FORMAT='("Below logHighLim (log, GT 0): ",I0)',N_ELEMENTS(WHERE(ALOG10(ABS(maximus.\1(WHERE(maximus.\1 GT 0)))) LE logHighLim))
  PRINT,''
  PRINT,FORMAT='("Above logHighLim (log, LT 0): ",I0)',N_ELEMENTS(WHERE(ALOG10(ABS(maximus.\1(WHERE(maximus.\1 LT 0)))) GT logHighLim))
  PRINT,FORMAT='("Below logHighLim (log, LT 0): ",I0)',N_ELEMENTS(WHERE(ALOG10(ABS(maximus.\1(WHERE(maximus.\1 LT 0)))) LE logHighLim))
